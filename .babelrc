{
  "presets": [
    [ "es2015", { "loose": true, "modules" : false } ],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "transform-react-remove-prop-types",
        "transform-runtime"
      ]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
